<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Parallel Test Suite" parallel="classes" thread-count="2">
    <test verbose="2" preserve-order="true" name="AllParallel">
        <classes>
            <class name="org.example.ParallelTestOne">
                <methods>
                    <include name="testGetFullName"/>
                    <include name="testGetAge"/>
                    <include name="testSetFirstAndLastNames"/>
                    <include name="testSetAge"/>
                </methods>
            </class>
            <class name="org.example.ParallelTestTwo">
                <methods>
                    <include name="testGetTitle"/>
                    <include name="testGetAuthor"/>
                    <include name="testGetPublisher"/>
                    <include name="testGetYear"/>
                    <include name="testGetPrice"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>